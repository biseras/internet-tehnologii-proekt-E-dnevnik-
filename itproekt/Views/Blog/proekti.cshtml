@model IEnumerable<itproekt.Table>

@{
    ViewBag.Title = "proekti";
}

<h2 style="color:darkblue">Известувања за проекти во кои може да се вклучат учениците</h2>


<table style="border-collapse:collapse">

    @foreach (var item in Model)
    {
        <tr>
            <td>
                <b style="color:brown; font-size:large">@Html.DisplayFor(modelItem => item.Btitle)</b>
            </td>
        </tr>
        <tr>
            <td></td>
        </tr>
        <tr>
            <td>
                Категорија: <strong style="color:brown">@Html.DisplayFor(modelItem => item.BCategory)</strong>
            </td>
        </tr>
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BDesc)
            </td>
        </tr>
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BPa)
            </td>
        </tr>
    }

</table>
